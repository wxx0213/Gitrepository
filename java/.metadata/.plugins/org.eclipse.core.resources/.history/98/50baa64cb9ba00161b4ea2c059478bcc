package Action;
import Dao.*;
public class UserManage 
{int userId;
public int getUserId() {
	return userId;
}
public void setUserId(int userId) {
	this.userId = userId;
}
public String deleteUserById(int userId)
{
	TbUserDao userDao=new TbUserDao();
	if(userId!=0)
	{
		userDao.DeleteById(userId);
	}
	return "success"; 
}
}
